Show Page For A List
<br><br>
<br><br>
<p><%= @list.content %></p>


<% if authorized_to_edit?(@list) %>
    <a href="/lists/<%= @list.id %>/edit">Edit Entry</a>
    <br><br>
    <form class="" action="/lists/<%= @list.id %>"
    method="post">
        <input type="hidden" name="_method" value="DELETE">
        <input type="submit" name="" value="Delete this Entry">
    </form>
<% end %>